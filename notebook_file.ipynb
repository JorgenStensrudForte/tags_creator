def gpt_analyse_grouped(word, list):
    # use gpt chat.completion to analyse the grouped description column
    prompt = f"""Bruk følgende liste med setninger: {list} for å lage en begrunnelse for hvorfor ordet {word} er brukt for å beskrive en tekst. skriv 50 ord
    """
    completion = openai.ChatCompletion.create(
        engine="chat",
        messages=[{"role": "user", "content": prompt}],
        temperature=0.7,
        max_tokens=150,
        top_p=0.95,
        frequency_penalty=0,
        presence_penalty=0,
        stop=None
        )
    return completion.choices[0].message.content
