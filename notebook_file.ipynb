        deployment_id=gpt_model,
        messages=[
            {
                "role": "user",
                "content": "{}".format(prompt),
            }
        ],
        temperature=0.3,
        max_tokens=1500,
        top_p=1.0,
        frequency_penalty=0.1,
        presence_penalty=0.1,
    )
    return str(completion.choices[0].message.content)
