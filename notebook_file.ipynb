                print(f"Error at index {index}: {e}")
                time.sleep(5)  # Wait for 5 seconds before retrying
            else:
                print(f"Skipping index {index} after 3 failed attempts")
                continue  # Skip the current index if the API call fails 3 times

    return df_tags

