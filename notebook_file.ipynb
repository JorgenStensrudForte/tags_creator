                elif re.search('technical (specifications|highlights|data)', line, re.IGNORECASE):                    
                    break

                if not in_features_section:
                    line = line.replace('®', '')  # Remove ® symbol
                    if line.startswith('•'):
                        text += '\n' + line  # Add bullet point to new line
